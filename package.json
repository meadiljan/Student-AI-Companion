{
  "name": "jade-bear-soar",
  "version": "1.0.0",
  "description": "Full-stack application with React frontend and Express backend",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:backend\" \"pnpm run dev:frontend\"",
    "dev:frontend": "pnpm -C frontend run dev",
    "dev:backend": "pnpm -C backend run dev",
    "build": "concurrently \"pnpm run build:frontend\" \"pnpm run build:backend\"",
    "build:frontend": "pnpm -C frontend run build",
    "build:backend": "pnpm -C backend run build",
    "start": "concurrently \"pnpm run start:backend\" \"pnpm run start:frontend\"",
    "start:frontend": "pnpm -C frontend run preview",
    "start:backend": "pnpm -C backend run start",
    "install:all": "pnpm install && pnpm -C frontend install && pnpm -C backend install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "react",
    "typescript",
    "express",
    "vite",
    "tailwindcss"
  ],
  "author": "",
  "license": "MIT"
}